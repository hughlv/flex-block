{
  "library": "FlexBlock",
  "libraryTarget": "umd",
  "sourceMap": true,
  "alias": {
    "@": "./src/components"
  },
  "plugins": [
    ["build-plugin-component-multiple", {
      "themePackage": "@alifd/theme-2"
    }],
    [
      "build-plugin-fusion",
      {
        "uniteBaseComponent": "@alifd/next",
        "importOptions": {
          "libraryDirectory": "lib"
        }
      }
    ]
  ]
}
